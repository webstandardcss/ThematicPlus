}

#blog-title, body.msie #blog-title {
	font-family: header-font !important;
}

#blog-description, h1#blog-description {
	font-family: lists-font !important;
	border:none;
	font-weight: 300;
	font-size: 16px;
}

#header-logo {
	position:absolute;
	top: -10px;
	left: -60px;
}

#comments, #respond {
	width: 80%;
	margin:0 auto;
	padding: 20px;
}

.clearfix {
	clear:both;
}

#header-quote {
	position:absolute;
	right: 0;
	top: 20px;
}
#header-quote img{
	height: 150px;
}


