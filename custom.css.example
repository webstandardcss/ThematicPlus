#header, #main, #footer, #branding, #container, #content {
	position:relative;
}

body {	
	background:#000000 url() fixed top center;
}

body.site, body.site input, body.site textarea {
	/* font-family:"Trebuchet MS", Verdana, Arial, Helvetica, sans-serif; */
	font-size:13px;
}

h1, h2, h3, h4, h5, h6 {
	color:#003;
	border-bottom: solid 2px #ccc;
	padding-bottom: 5px;
	margin-bottom: 5px;
}

#primary h1, #primary h2, #primary h3, #primary h4, #primary h5, #primary h6, #secondary h1, #secondary h2, #secondary h3, #secondary h4, #secondary h5, #secondary h6, body.msie #blog-description  {
	color:#ccc;
}

#content {
	left: 32px;
}

body.wordpress h3 { 
	font-size: 21px;
}

#content h3 { 
	font-size: 18px;
}

#content h3 + h4 { 

}

label {
	font-weight: 900;
	font-size: 120%;
}

#content h4 { 
	color: #602F11;
}

body.one-column #container {
	width: 960px;
	background-color:#ccc;
	margin:0 auto;
	padding:0;
}

.picture-row + h3 {
	margin-top: -21px;
}

a {
	color:#00c;
}

a:hover {
	border-bottom: solid 1px black;
}

a:active {
	color:#006;
}

.entry-content {
	padding-top: 0px;
}

.entry-meta, .entry-meta a, .entry-utility, .entry-utility a, #login, #login a, #form-allowed-tags {
